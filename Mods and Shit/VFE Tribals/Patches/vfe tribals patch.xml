<?xml version="1.0" encoding="utf-8" ?>
<Patch>          

<!-- VFET patches -->

<!-- INTELLECTUAL -->
<Operation Class="PatchOperationAdd">
  <xpath>Defs/SkillDef[defName="Intellectual"]/modExtensions/li[@Class="ProgressionEducation.SkillBuildingRequirement"]/requiredBuildings</xpath>
  <value>
    <li>VFET_ResearchSpot</li>
  </value>
</Operation>

<!-- CRIB RESEARCH -->
<Operation Class="PatchOperationReplace">
  <xpath>Defs/ThingDef[defName="TribalCrib"]/researchPrerequisites</xpath>
  <value>
    <researchPrerequisites Inherit="False">
      <li>VFET_Furniture</li>
    </researchPrerequisites>  
  </value>
</Operation>


</Patch>