    <ThingDef ParentName="BuildingBase">
        <defName>PE_ShootingTarget</defName>
        <label>practice target</label>
        <description>A target for practicing shooting skills.</description>
        <statBases>
            <MaxHitPoints>100</MaxHitPoints>
            <WorkToBuild>1000</WorkToBuild>
            <Mass>15</Mass>
            <Flammability>1.0</Flammability>
            <Beauty>-5</Beauty>
        </statBases>
        <stuffCategories>
            <li>Metallic</li>
            <li>Woody</li>
        </stuffCategories>
        <researchPrerequisites>
            <li>Ferny_IndustrialEducation</li>
        </researchPrerequisites>
        <costStuffCount>50</costStuffCount>
        <altitudeLayer>BuildingOnTop</altitudeLayer>
        <passability>PassThroughOnly</passability>
        <pathCost>42</pathCost>
        <fillPercent>0.4</fillPercent>
        <hasInteractionCell>True</hasInteractionCell>
        <interactionCellOffset>(0,0,-2)</interactionCellOffset>
        <designationCategory>Ferny_Education</designationCategory>
        <placeWorkers>
            <li>PlaceWorker_ShowFacilitiesConnections</li>
        </placeWorkers>
        <size>(1,5)</size>
        <graphicData>
            <texPath>Buildings/Tools/ShootingTarget</texPath>
            <graphicClass>Graphic_Multi</graphicClass>
            <drawSize>(1,1)</drawSize>
            <drawOffsetNorth>(0, 0, 2)</drawOffsetNorth>
            <drawOffsetSouth>(0, 0, -2)</drawOffsetSouth>
            <drawOffsetEast>(2, 0, 0)</drawOffsetEast>
            <drawOffsetWest>(-2, 0, 0)</drawOffsetWest>
            <damageData>
                <cornerTL>Damage/Corner</cornerTL>
                <cornerTR>Damage/Corner</cornerTR>
                <cornerBL>Damage/Corner</cornerBL>
                <cornerBR>Damage/Corner</cornerBR>
            </damageData>
        </graphicData>
    </ThingDef>

</Defs>